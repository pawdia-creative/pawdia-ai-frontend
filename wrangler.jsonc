{
  // 这个文件明确告诉 Cloudflare 这是一个 Pages 部署
  "name": "pawdia-ai-frontend",
  "compatibility_date": "2025-12-12",
  // 使用 Pages 项目配置
  "pages_build_output_dir": "./frontend-separation/dist",
  // 静态资产配置
  "assets": {
    "directory": "./frontend-separation/dist",
    "binding": "ASSETS"
  },
  // 禁用 Workers 脚本功能
  "no_worker": true
}